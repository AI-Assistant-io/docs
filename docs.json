{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "AI Assistant",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      },
      {
        "tab": "Release Notes",
        "groups": [
          {
            "group": "Engineering",
            "pages": [
              "release-notes/engineering/index",
              "release-notes/engineering/v2.0.0",
              "release-notes/engineering/v1.9.1",
              "release-notes/engineering/v1.9.0",
              "release-notes/engineering/v1.8.1",
              "release-notes/engineering/v1.8.0",
              "release-notes/engineering/v1.7.0",
              "release-notes/engineering/v1.6.0",
              "release-notes/engineering/v1.5.0",
              "release-notes/engineering/v1.4.0",
              "release-notes/engineering/v1.3.0",
              "release-notes/engineering/v1.2.0",
              "release-notes/engineering/v1.1.0",
              "release-notes/engineering/v1.0.0"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://ai-assistant.io",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@ai-assistant.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://ai-assistant.io"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/AI-Assistant-io"
    }
  }
}
